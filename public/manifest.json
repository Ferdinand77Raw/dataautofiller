{
  "manifest_version": 3,
  "name": "Data Autofiller",
  "version": "1.0.1",
  "description": "Save time by automating data filling from one webform to other webform",
  "action": {
    "default_icon": "./img/icon128.png",
    "persistent": true,
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab", 
    "scripting"
  ],
  "icons": {
    "32": "./img/icon32.png",
    "48": "./img/icon48.png",
    "64": "./img/icon64.png",
    "128":"./img/icon128.png",
    "256": "./img/icon256.png"
  }   
}